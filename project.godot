; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="GodotInk"
config/features=PackedStringArray("4.0", "C#")
config/icon="res://icon.png"

[audio]

driver/enable_input=true

[display]

window/size/viewport_width=480
window/size/viewport_height=320
window/size/resizable=false

[dotnet]

project/assembly_name="GodotInk"

[editor]

export/convert_text_resources_to_binary=true

[editor_plugins]

enabled=PackedStringArray("res://addons/GodotInk/plugin.cfg")

